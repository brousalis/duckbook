{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon48.png"
   },
   "content_scripts": [ {
      "js": [ "duckbook.js" ],
      "matches": [ "*://*.facebook.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "duckbook",
   "icons": {
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "duckbook",
   "permissions": [ "activeTab", "storage" ],
   "version": "1.0.0"
}
